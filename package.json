{
  "name": "alec-rust",
  "version": "1.1.4",
  "private": true,
  "description": "Alec Rust personal portfolio and blog.",
  "readmeFilename": "README.md",
  "repository": "AlecRust/alec-rust",
  "author": "Alec Rust",
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "start": "docker-compose up",
    "devon": "cfcli devmode on -d alecrust.com",
    "devoff": "cfcli devmode off -d alecrust.com",
    "deploy": "git push -v production refs/heads/master:refs/heads/master; npm run purgecache",
    "purgecache": "cfcli purgecache -d alecrust.com",
    "wp": "wpcmd() { docker exec $(docker ps -lq) /bin/bash -c \"sudo -u www-data wp $(echo $@)\"; };wpcmd "
  },
  "dependencies": {
    "normalize.css": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "del": "^2.2.2",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-bump": "^2.7.0",
    "gulp-cache": "^0.4.5",
    "gulp-clip-empty-files": "^0.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-git": "^2.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-postcss": "^6.3.0",
    "gulp-stylint": "^4.0.0",
    "gulp-stylus": "^2.6.0",
    "gulp-touch": "^1.0.1",
    "gulp-uglify": "^2.0.1",
    "postcss-bem-linter": "^2.6.0",
    "postcss-calc": "^5.3.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-import": "^9.1.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  }
}
