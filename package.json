{
  "name": "alec-rust",
  "version": "1.1.2",
  "private": true,
  "description": "Alec Rust personal portfolio and blog.",
  "readmeFilename": "README.md",
  "repository": "AlecRust/alec-rust",
  "author": "Alec Rust",
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "start": "docker-compose up",
    "devon": "cfcli devmode on -d alecrust.com",
    "devoff": "cfcli devmode off -d alecrust.com",
    "deploy": "git push -v production refs/heads/master:refs/heads/master; npm run purgecache",
    "purgecache": "cfcli purgecache -d alecrust.com",
    "wp": "wpcmd() { docker exec $(docker ps -lq) /bin/bash -c \"sudo -u www-data wp $(echo $@)\"; };wpcmd "
  },
  "dependencies": {
    "normalize.css": "^4.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "curlrequest": "^0.5.7",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.5.1",
    "gulp-cache": "^0.4.5",
    "gulp-clip-empty-files": "^0.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.3",
    "gulp-jshint": "^2.0.1",
    "gulp-notify": "^2.2.0",
    "gulp-postcss": "^6.2.0",
    "gulp-size": "^2.1.0",
    "gulp-stylint": "^3.0.0",
    "gulp-stylus": "^2.5.0",
    "gulp-uglify": "^2.0.0",
    "jshint": "^2.9.3",
    "jshint-stylish": "^2.2.1",
    "postcss-at2x": "^2.0.0",
    "postcss-bem-linter": "^2.5.1",
    "postcss-calc": "^5.3.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.1.2",
    "require-dir": "^0.3.0"
  }
}
