    "gulp-bump": "^2.7.0",
    "gulp-clip-empty-files": "^0.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-git": "^2.4.2",
    "gulp-postcss": "^7.0.0",
    "gulp-stylint": "^4.0.0",
    "gulp-stylus": "^2.6.0",
    "gulp-touch": "^1.0.1",
    "gulp-uglify": "^3.0.0",
    "postcss-bem-linter": "^3.0.0",
    "postcss-calc": "^6.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.1.0",
    "postcss-import": "^11.0.0"
